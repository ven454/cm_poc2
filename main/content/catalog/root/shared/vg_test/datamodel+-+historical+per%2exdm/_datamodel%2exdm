                                    WHEN D2.c24 IS NOT NULL THEN D2.c24
                                END ASC,
                                CASE
                                    WHEN D1.c25 IS NOT NULL THEN D1.c25
                                    WHEN D2.c25 IS NOT NULL THEN D2.c25
                                END ASC,
                                CASE
                                    WHEN D1.c26 IS NOT NULL THEN D1.c26
                                    WHEN D2.c26 IS NOT NULL THEN D2.c26
                                END ASC,
                                CASE
                                    WHEN D1.c27 IS NOT NULL THEN D1.c27
                                    WHEN D2.c27 IS NOT NULL THEN D2.c27
                                END ASC,
                                CASE
                                    WHEN D1.c28 IS NOT NULL THEN D1.c28
                                    WHEN D2.c28 IS NOT NULL THEN D2.c28
                                END ASC,
                                CASE
                                    WHEN D1.c29 IS NOT NULL THEN D1.c29
                                    WHEN D2.c29 IS NOT NULL THEN D2.c29
                                END ASC,
                                CASE
                                    WHEN D1.c30 IS NOT NULL THEN D1.c30
                                    WHEN D2.c30 IS NOT NULL THEN D2.c30
                                END ASC,
                                CASE
                                    WHEN D1.c31 IS NOT NULL THEN D1.c31
                                    WHEN D2.c31 IS NOT NULL THEN D2.c31
                                END ASC,
                                CASE
                                    WHEN D1.c32 IS NOT NULL THEN D1.c32
                                    WHEN D2.c32 IS NOT NULL THEN D2.c32
                                END ASC,
                                CASE
                                    WHEN D1.c33 IS NOT NULL THEN D1.c33
                                    WHEN D2.c33 IS NOT NULL THEN D2.c33
                                END ASC,
                                CASE
                                    WHEN D1.c34 IS NOT NULL THEN D1.c34
                                    WHEN D2.c34 IS NOT NULL THEN D2.c34
                                END ASC,
                                CASE
                                    WHEN D1.c35 IS NOT NULL THEN D1.c35
                                    WHEN D2.c35 IS NOT NULL THEN D2.c35
                                END ASC,
                                CASE
                                    WHEN D1.c36 IS NOT NULL THEN D1.c36
                                    WHEN D2.c36 IS NOT NULL THEN D2.c36
                                END ASC,
                                CASE
                                    WHEN D1.c37 IS NOT NULL THEN D1.c37
                                    WHEN D2.c37 IS NOT NULL THEN D2.c37
                                END ASC,
                                CASE
                                    WHEN D1.c38 IS NOT NULL THEN D1.c38
                                    WHEN D2.c38 IS NOT NULL THEN D2.c38
                                END ASC,
                                CASE
                                    WHEN D1.c39 IS NOT NULL THEN D1.c39
                                    WHEN D2.c39 IS NOT NULL THEN D2.c39
                                END ASC,
                                CASE
                                    WHEN D1.c40 IS NOT NULL THEN D1.c40
                                    WHEN D2.c40 IS NOT NULL THEN D2.c40
                                END ASC,
                                CASE
                                    WHEN D1.c41 IS NOT NULL THEN D1.c41
                                    WHEN D2.c41 IS NOT NULL THEN D2.c41
                                END ASC,
                                CASE
                                    WHEN D1.c42 IS NOT NULL THEN D1.c42
                                    WHEN D2.c42 IS NOT NULL THEN D2.c42
                                END ASC)
                            AS c51
                   FROM SAWITH0  D1
                        FULL OUTER JOIN SAWITH3 D2
                            ON     D1.c25 = D2.c25
                               AND D1.c24 = D2.c24
                               AND D1.c38 = D2.c38
                               AND D1.c6 = D2.c6
                               AND D1.c5 = D2.c5
                               AND D1.c37 = D2.c37
                               AND SYS_OP_MAP_NONNULL (D1.c34) =
                                       SYS_OP_MAP_NONNULL (D2.c34)
                               AND SYS_OP_MAP_NONNULL (D1.c44) =
                                       SYS_OP_MAP_NONNULL (D2.c44)
                               AND SYS_OP_MAP_NONNULL (D1.c45) =
                                       SYS_OP_MAP_NONNULL (D2.c45)
                               AND D1.c31 = D2.c31
                               AND SYS_OP_MAP_NONNULL (D1.c35) =
                                       SYS_OP_MAP_NONNULL (D2.c35)
                               AND SYS_OP_MAP_NONNULL (D1.c46) =
                                       SYS_OP_MAP_NONNULL (D2.c46)
                               AND SYS_OP_MAP_NONNULL (D1.c47) =
                                       SYS_OP_MAP_NONNULL (D2.c47)
                               AND D1.c23 = D2.c23
                               AND D1.c16 = D2.c16
                               AND D1.c9 = D2.c9
                               AND D1.c15 = D2.c15
                               AND D1.c41 = D2.c41
                               AND D1.c17 = D2.c17
                               AND D1.c19 = D2.c19
                               AND SYS_OP_MAP_NONNULL (D1.c10) =
                                       SYS_OP_MAP_NONNULL (D2.c10)
                               AND D1.c39 = D2.c39
                               AND SYS_OP_MAP_NONNULL (D1.c12) =
                                       SYS_OP_MAP_NONNULL (D2.c12)
                               AND D1.c8 = D2.c8
                               AND SYS_OP_MAP_NONNULL (D1.c13) =
                                       SYS_OP_MAP_NONNULL (D2.c13)
                               AND D1.c22 = D2.c22
                               AND SYS_OP_MAP_NONNULL (D1.c14) =
                                       SYS_OP_MAP_NONNULL (D2.c14)
                               AND SYS_OP_MAP_NONNULL (D1.c11) =
                                       SYS_OP_MAP_NONNULL (D2.c11)
                               AND SYS_OP_MAP_NONNULL (D1.c21) =
                                       SYS_OP_MAP_NONNULL (D2.c21)
                               AND SYS_OP_MAP_NONNULL (D1.c29) =
                                       SYS_OP_MAP_NONNULL (D2.c29)
                               AND SYS_OP_MAP_NONNULL (D1.c29) =
                                       SYS_OP_MAP_NONNULL (D2.c29)
                               AND SYS_OP_MAP_NONNULL (D1.c30) =
                                       SYS_OP_MAP_NONNULL (D2.c30)
                               AND SYS_OP_MAP_NONNULL (D1.c40) =
                                       SYS_OP_MAP_NONNULL (D2.c40)
                               AND D1.c20 = D2.c20
                               AND SYS_OP_MAP_NONNULL (D1.c42) =
                                       SYS_OP_MAP_NONNULL (D2.c42)
                               AND D1.c28 = D2.c28
                               AND D1.c26 = D2.c26
                               AND D1.c27 = D2.c27
                               AND D1.c7 = D2.c7
                               AND D1.c43 = D2.c43) D1
          WHERE (D1.c51 = 1))
  SELECT D1.c1 AS "Object",
         D1.c2 AS "Object Description",
         D1.c3 AS "Activity",
         D1.c4 AS "Account Status Desc",
         D1.c5 AS "Account Type Desc",
         D1.c6 AS "Acct Account Group Desc",
         D1.c7 AS "Acct End Date",
         D1.c8 AS "Acct Start Date",
         D1.c9 AS "Anticip End Date",
         D1.c10 AS "Anticip Start Date",
         D1.c11 AS "Principal Investigator",
         D1.c12 AS "Resp Org Cd",
         D1.c13 AS "Resp Org Desc",
         D1.c14 AS "Resp Org",
         D1.c15 AS "Resp Tub Cd",
         D1.c16 AS "Resp Tub",
         D1.c17 AS "Spon Award",
         D1.c18 AS "Sponsor",
         D1.c19 AS "Fund",
         D1.c21 AS "Object Num Name",
         D1.c22 AS "Org",
         D1.c23 AS "Subactivity Description",
         D1.c24 AS "Subactivity",
         D1.c25 AS "Tub",
         D1.c26 AS "Budgets",
         D1.c27 AS "PJTD_Expense",
         D1.c28 AS "PJTD_Income",
         D1.c29 AS "Total_Authorized",
         D1.c30 AS "Total_Carry_Fwd",
         D1.c31 AS "Total_Obligated",
         D1.c32 AS "Remaining_GL_Budget",
         D1.c33 AS "Object_Grp",
         D1.c34 AS "Account",
         D1.c35 AS "Per_GLBud_Expd",
         D1.c36 AS "Asterisk",
         D1.c37 AS "F_A_Flag",
         D1.c38 AS "NON_OP_Flag",
         D1.c39 AS "PJTD_NonOper",
         D1.c40 AS "Chart_of_Accounts",
         D1.c41 AS "Super_Obj",
         D1.c42 AS "Super_Obj_Name",
         D1.c43 AS "Count_NonOper",
         D1.c44 AS "Count_Income",
         D1.c45 AS "SubAct_Amt",
         D1.c46 AS "FY_Expense",
         D1.c47 AS "PJTD_Exp",
         D1.c48 AS "Indirect_Cost_Basis_Cd",
         D1.c49 AS "Indirect_Cost_Basis_Rate",
         D1.c50 AS "Project_NickName"
    FROM SAWITH4 D1
ORDER BY c19, -- fund
         c3,  -- Activity 
         c24,  -- Sub activity
         c25, -- Tub
         c22,  -- Org
         c1   -- Object Code]]>
         </sql>
      </dataSet>
      <dataSet name="XDO" type="complex">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[select	 "OBI Users"."User Id" as "User Id",
	 "OBI Users"."User Desc" as "User Desc" 
 from	"OBI Security"."OBI Users" "OBI Users"
where "OBI Users"."User Id" = :xdo_user_name]]>
         </sql>
      </dataSet>
      <dataSet name="G_PeriodTrx_PJTD_Exp_End" type="complex">
         <sql dataSourceRef="dwcen">
            <![CDATA[SELECT          T568689.TUB                       AS "TUB",
                  T568693.ORG                       AS "ORG",
                  T568686.FUND                      AS "FUND",
                  T568669.OBJECT                    AS "OBJECT",
                  T568685.ACTIVITY                  AS "ACTIVITY",
                  T568677.SUBACTIVITY               AS "SUBACTIVITY" ,
SUM (T589413.ACTUAL_NET_PJTD_END) AS "PJTD_Exp_End"
             FROM DWGL.DGL_COA_TUBS_D              T568689 /* B1_DGL_COA_TUBS_D (Grants) */
                                                          ,
                  DWGL.DGL_COA_SUBACTIVITIES_D     T568677 /* B1_DGL_COA_SUBACTIVITIES_D (Grants) */
                                                          ,
                  DWGL.DGL_COA_ORGS_D              T568693 /* B1_DGL_COA_ORGS_D (Grants) */
                                                          ,
                  DWGL.DGL_COA_OBJECTS_D           T568669 /* B1_DGL_COA_OBJECTS_D (Grants) */
                                                          ,
                  DWGL.DGL_COA_FUNDS_D             T568686 /* B1_DGL_COA_FUNDS_D (Grants) */
                                                          ,
                  DWGL.DGL_COA_ACTIVITIES_D        T568685 /* B1_DGL_COA_ACTIVITIES_D (Grants) */
                                                          ,
                  DWGL.DGL_PERIODS_D               T568714 /* B1_DGL_PERIODS_D (Grants) */
                                                          ,
                  ODSGM.OGM_ACCOUNTS               T568662 /* B2_OGM_ACCOUNTS (Grants - PER inner) */
                                                          ,
            --      ODSGM.OGM_SEC_USER_RPT_SEGMENTS  T589369 , /* B2_OGM_SEC_USER_RPT_SEGMENTS (PER Security Join) */                                                          
                  HUDW.DGM_CURR_PER_GL_BALS_F_ALL_V T589413 /* D1_DGM_CURR_PER_GL_BALS_F_ALL_V (Grants - PER open view) */
            WHERE (    T568662.DW_GL_FUND_KEY = T589413.DW_FUND_KEY
                   AND T568662.DW_ACCOUNT_KEY = T589413.DW_ACCOUNT_KEY
          --         AND T568662.DW_SEGMENT_KEY = T589369.DW_SEGMENT_KEY
                   AND T568669.ENABLED_FLAG = 'Y'
      /*             AND T568669.OBJECT = '7654'
                   AND T568677.SUBACTIVITY = '0010'
                   AND T568685.ACTIVITY = '257832'
                   AND T568686.FUND = '258361'
                   AND T568689.TUB = '275'
                   AND T568693.ORG = '23440'    */        
                   AND T568669.OBJECT_KEY = T589413.DW_OBJECT_KEY
                   AND T568677.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')
                   AND T568677.SUBACTIVITY_KEY = T589413.DW_SUBACTIVITY_KEY
                   AND T568685.ACTIVITY_KEY = T589413.DW_ACTIVITY_KEY
                   AND T568685.ACTIVITY_TYPE_CODE = 'S'
                   AND T568685.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')
                   AND T568686.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')
                   AND T568686.FUND_KEY = T589413.DW_FUND_KEY
                   AND T568689.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')                 
                   AND T568689.TUB_KEY = T589413.DW_TUB_KEY
                   AND T568693.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')                   
                   AND T568693.ORG_KEY = T589413.DW_ORG_KEY
                   AND T568714.PERIOD_KEY = T589413.DW_PERIOD_KEY
                   AND T568714.PERIOD_KEY = :PeriodToKey
          --         AND T589369.USER_UNIV_ID = '31240778'
                   AND T589413.DW_PERIOD_KEY = :PeriodToKey
                   AND T568714.CURRENT_PERIOD_OFFSET < 1
                   AND 0 < T568714.PERIOD_KEY
                   AND T568669.OBJECT >= '4000')
         GROUP BY T568669.OBJECT,
                  T568677.SUBACTIVITY,
                  T568685.ACTIVITY,
                  T568686.FUND,
                  T568689.TUB,
                  T568693.ORG]]>
         </sql>
      </dataSet>
      <dataSet name="From Period" type="complex">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[SELECT
"Grants Management"."Dim - Periods"."Accounting Start Date" AS "From_Period_Start_Dt",
"Grants Management"."Dim - Periods"."Accounting End Date" AS "From_Period_End_Dt"
FROM "Grants Management"
WHERE
"Dim - Periods"."Period Key" = :PeriodFromKey]]>
         </sql>
      </dataSet>
      <dataSet name="ToPeriod" type="complex">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[SELECT
"Grants Management"."Dim - Periods"."Accounting Start Date" AS "To_Period_Start_Dt",
"Grants Management"."Dim - Periods"."Accounting End Date" AS "To_Period_End_Dt"
FROM "Grants Management"
WHERE
"Dim - Periods"."Period Key" = :PeriodToKey]]>
         </sql>
      </dataSet>
      <dataSet name="G_PeriodTrx_PJTD_Exp_Begin" type="complex">
         <sql dataSourceRef="dwcen">
            <![CDATA[SELECT            T568689.TUB                       AS "TUB",
                  T568693.ORG                       AS "ORG",
                  T568686.FUND                      AS "FUND",
                  T568669.OBJECT                    AS "OBJECT",
                  T568685.ACTIVITY                  AS "ACTIVITY",
                  T568677.SUBACTIVITY               AS "SUBACTIVITY" ,
SUM (T589413.ACTUAL_NET_PJTD_BEGIN) AS "PJTD_Exp_Begin"
FROM DWGL.DGL_COA_TUBS_D              T568689 /* B1_DGL_COA_TUBS_D (Grants) */
                                                          ,
                  DWGL.DGL_COA_SUBACTIVITIES_D     T568677 /* B1_DGL_COA_SUBACTIVITIES_D (Grants) */
                                                          ,
                  DWGL.DGL_COA_ORGS_D              T568693 /* B1_DGL_COA_ORGS_D (Grants) */
                                                          ,
                  DWGL.DGL_COA_OBJECTS_D           T568669 /* B1_DGL_COA_OBJECTS_D (Grants) */
                                                          ,
                  DWGL.DGL_COA_FUNDS_D             T568686 /* B1_DGL_COA_FUNDS_D (Grants) */
                                                          ,
                  DWGL.DGL_COA_ACTIVITIES_D        T568685 /* B1_DGL_COA_ACTIVITIES_D (Grants) */
                                                          ,
                  DWGL.DGL_PERIODS_D               T568714 /* B1_DGL_PERIODS_D (Grants) */
                                                          ,
                  ODSGM.OGM_ACCOUNTS               T568662 /* B2_OGM_ACCOUNTS (Grants - PER inner) */
                                                          ,
            --      ODSGM.OGM_SEC_USER_RPT_SEGMENTS  T589369 , /* B2_OGM_SEC_USER_RPT_SEGMENTS (PER Security Join) */                                                          
                  HUDW.DGM_CURR_PER_GL_BALS_F_ALL_V T589413 /* D1_DGM_CURR_PER_GL_BALS_F_ALL_V (Grants - PER open view) */
            WHERE (    T568662.DW_GL_FUND_KEY = T589413.DW_FUND_KEY
                   AND T568662.DW_ACCOUNT_KEY = T589413.DW_ACCOUNT_KEY
          --         AND T568662.DW_SEGMENT_KEY = T589369.DW_SEGMENT_KEY
                   AND T568669.ENABLED_FLAG = 'Y'
      /*             AND T568669.OBJECT = '7654'
                   AND T568677.SUBACTIVITY = '0010'
                   AND T568685.ACTIVITY = '257832'
                   AND T568686.FUND = '258361'
                   AND T568689.TUB = '275'
                   AND T568693.ORG = '23440'    */        
                   AND T568669.OBJECT_KEY = T589413.DW_OBJECT_KEY
                   AND T568677.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')
                   AND T568677.SUBACTIVITY_KEY = T589413.DW_SUBACTIVITY_KEY
                   AND T568685.ACTIVITY_KEY = T589413.DW_ACTIVITY_KEY
                   AND T568685.ACTIVITY_TYPE_CODE = 'S'
                   AND T568685.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')
                   AND T568686.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')
                   AND T568686.FUND_KEY = T589413.DW_FUND_KEY
                   AND T568689.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')                 
                   AND T568689.TUB_KEY = T589413.DW_TUB_KEY
                   AND T568693.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')                   
                   AND T568693.ORG_KEY = T589413.DW_ORG_KEY
                   AND T568714.PERIOD_KEY = T589413.DW_PERIOD_KEY
                   AND T568714.PERIOD_KEY = :PeriodFromKey
          --         AND T589369.USER_UNIV_ID = '31240778'
                   AND T589413.DW_PERIOD_KEY = :PeriodFromKey
                   AND T568714.CURRENT_PERIOD_OFFSET < 1
                   AND 0 < T568714.PERIOD_KEY
                   AND T568669.OBJECT >= '4000')
         GROUP BY T568669.OBJECT,
                  T568677.SUBACTIVITY,
                  T568685.ACTIVITY,
                  T568686.FUND,
                  T568689.TUB,
                  T568693.ORG]]>
         </sql>
      </dataSet>
      <dataSet name="Root_Concat" type="complex">
         <sql dataSourceRef="dwcen">
            <![CDATA[SELECT DISTINCT 
          T568689.TUB                       AS "TUB",
                  T568693.ORG                       AS "ORG",
                  T568686.FUND                      AS "FUND",
                  T568685.ACTIVITY                  AS "ACTIVITY",
                  T568677.SUBACTIVITY               AS "SUBACTIVITY" ,
         T568641.ROOT                AS "Root",
         SUM (T589413.EXP_ACTUAL_PJTD) AS  "DO NOT USE"
    FROM DWGL.DGL_COA_TUBS_D              T568689 /* B1_DGL_COA_TUBS_D (Grants) */
                                                 ,
         DWGL.DGL_COA_SUBACTIVITIES_D     T568677 /* B1_DGL_COA_SUBACTIVITIES_D (Grants) */
                                                 ,
         DWGL.DGL_COA_ROOTS_D             T568641 /* B1_DGL_COA_ROOTS_D (Grants) */
                                                 ,
         DWGL.DGL_COA_ORGS_D              T568693 /* B1_DGL_COA_ORGS_D (Grants) */
                                                 ,
         DWGL.DGL_COA_FUNDS_D             T568686 /* B1_DGL_COA_FUNDS_D (Grants) */
                                                 ,
         DWGL.DGL_COA_ACTIVITIES_D        T568685 /* B1_DGL_COA_ACTIVITIES_D (Grants) */
                                                 ,
         DWGL.DGL_PERIODS_D               T568714 /* B1_DGL_PERIODS_D (Grants) */
                                                 ,
         ODSGM.OGM_ACCOUNTS               T568662 /* B2_OGM_ACCOUNTS (Grants - PER inner) */
                                                 ,
     /*   ODSGM.OGM_SEC_USER_RPT_SEGMENTS  T589369 , */ /* B2_OGM_SEC_USER_RPT_SEGMENTS (PER Security Join) */  
            HUDW.DGM_CURR_PER_GL_BALS_F_ALL_V T589413 /* D1_DGM_CURR_PER_GL_BALS_F_ALL_V (Grants - PER open view) */
   WHERE (    T568641.END_DATE_ACTIVE =
                  TO_DATE ('9999-12-31 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
          AND T568641.ROOT_KEY = T589413.DW_ROOT_KEY
          AND T568662.DW_GL_FUND_KEY = T589413.DW_FUND_KEY
          AND T568662.DW_ACCOUNT_KEY = T589413.DW_ACCOUNT_KEY
      /*    AND T568662.DW_SEGMENT_KEY = T589369.DW_SEGMENT_KEY */
          AND T568677.END_DATE_ACTIVE =
                  TO_DATE ('9999-12-31 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
          AND T568677.SUBACTIVITY_KEY = T589413.DW_SUBACTIVITY_KEY
          AND T568685.ACTIVITY_KEY = T589413.DW_ACTIVITY_KEY
          AND T568685.END_DATE_ACTIVE =
                  TO_DATE ('9999-12-31 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
          AND T568686.END_DATE_ACTIVE =
                  TO_DATE ('9999-12-31 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
           AND T568686.FUND_KEY = T589413.DW_FUND_KEY
          AND T568689.END_DATE_ACTIVE =
                  TO_DATE ('9999-12-31 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
           AND T568689.TUB_KEY = T589413.DW_TUB_KEY
          AND T568693.END_DATE_ACTIVE =
                  TO_DATE ('9999-12-31 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
          AND T568693.ORG_KEY = T589413.DW_ORG_KEY
          AND T568714.PERIOD_KEY = T589413.DW_PERIOD_KEY
      --    AND T589369.USER_UNIV_ID = '31240778'
          AND T568714.CURRENT_PERIOD_OFFSET < 1
          AND 0 < T568714.PERIOD_KEY
          AND T568714.PERIOD_KEY BETWEEN :PeriodFromKey AND :PeriodToKey
          AND T589413.DW_PERIOD_KEY BETWEEN :PeriodFromKey AND :PeriodToKey )
GROUP BY T568641.ROOT,
         T568677.SUBACTIVITY,
         T568685.ACTIVITY,
         T568686.FUND,
         T568689.TUB,
         T568693.ORG]]>
         </sql>
      </dataSet>
