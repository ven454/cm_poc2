                                          '-'),
                                      T568054.SUBACTIVITY),
                                  '-'),
                              T568064.TUB),
                          '-'),
                      T568069.ORG)
                      AS c36,
                  T568046.SUPER_OBJECT                   AS c37,
                  T568046.SUPER_OBJECT_NAME              AS c38,
                  T568040.ALLOCATED_AMT                  AS c39,
                  T568040.ACCT_IDC_BASIS_CD              AS c40,
                  T608824.AIR_RATE                      AS c41,
                  T568029.NICKNAME                       AS c42,
                  T568054.ACTIVITY                       AS c43,
                  T568046.TDC_OVERHEAD_FLAG              AS c44,
                  T568046.HHSM_OVERHEAD_FLAG             AS c45,
                  T568046.MTDC_OVERHEAD_FLAG             AS c46,
                  T568046.DIR_INDIR_COST_FLAG            AS c47
             FROM DWGL.DGL_COA_ROOTS_D            T568021 /* B1_DGL_COA_ROOTS_D (Grants) */
                                                         ,
                  DWGL.DGL_COA_ORGS_D             T568069 /* B1_DGL_COA_ORGS_D (Grants) */
                                                         ,
                  DWGL.DGL_COA_OBJECTS_D          T568046 /* B1_DGL_COA_OBJECTS_D (Grants) */
                                                         ,
                  DWGL.DGL_COA_FUNDS_D            T568061 /* B1_DGL_COA_FUNDS_D (Grants) */
                                                         ,
                  DWGL.DGL_PERIODS_D              T568091 /* B1_DGL_PERIODS_D (Grants) */
                                                         ,
                                                         DWGL.DGL_PERIODS_D               DGP,    
                  DWGL.DGL_COA_TUBS_D             T568064 /* B1_DGL_COA_TUBS_D (Grants) */
                                                         ,
                  DWGL.DGL_COA_SUBACTIVITIES_D    T568054 /* B1_DGL_COA_SUBACTIVITIES_D (Grants) */
                                                         ,
                  DWGL.DGL_COA_ACTIVITIES_D       T568060 /* B1_DGL_COA_ACTIVITIES_D (Grants) */
                                                         ,
                  ODSGM.OGM_ACCOUNTS              T568040 /* B2_OGM_ACCOUNTS (Grants - PER inner) */
                  LEFT OUTER JOIN
                  (Select dw_account_key, dw_segment_key, air_rate, air_effective_date 
from 
(
select dw_account_key, dw_segment_key, air_rate, air_effective_date, 
Max(air_Effective_Date) over (PARTITION by rates.dw_account_key) as maxDate 
from odsgm.ogm_account_idc_rates rates 
where 
rates.air_effective_date <= sysdate AND 
rates.dw_active_flag = 'Y' AND 
rates.AIR_DELETION_DATE is NULL
) 
where air_effective_date = maxDate) T608824 On T568040.DW_ACCOUNT_KEY = T608824.DW_ACCOUNT_KEY,
                  HUDW.DGM_CURR_PERIOD_GL_BALS_F_V T573797 /* C1_DGM_CURR_PERIOD_GL_BALS_F_V (Grants - PER) */
                                                          ,
                                                           HUDW.DGM_CURR_PER_GL_BALS_F_ALL_V BUDGETS,       
                  ODSGM.OGM_SEC_USER_RPT_SEGMENTS T573798 /* B2_OGM_SEC_USER_RPT_SEGMENTS (PER Security Join) */
                                                         ,
                  ODSGM.OGM_SEGMENTS              T568029 /* B2_OGM_SEGMENTS (Grants - PER inner) */
            WHERE (    T568029.DW_SEGMENT_KEY = T568040.DW_SEGMENT_KEY
                   AND T568040.DW_GL_FUND_KEY = T573797.DW_FUND_KEY
                   AND T568040.DW_ACCOUNT_KEY = T573797.DW_ACCOUNT_KEY
                   AND T568040.DW_SEGMENT_KEY = T573798.DW_SEGMENT_KEY
                   AND T568046.ENABLED_FLAG = 'Y'
                   /*   AND T568046.OBJECT = '4310'   Test */
                   AND T568046.OBJECT_KEY = T573797.DW_OBJECT_KEY
                   AND T568054.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')
                   AND T568054.SUBACTIVITY_KEY = T573797.DW_SUBACTIVITY_KEY
                   AND T568060.ACTIVITY_KEY = T573797.DW_ACTIVITY_KEY
                   AND T568060.ACTIVITY_TYPE_CODE = 'S'
                   AND T568060.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')
                   AND T568061.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')
                   AND &lp_AcctSponFund
                   AND &lp_AcctTub
                   AND &lp_AcctOrg
                   AND &lp_AcctSponActivity
                   AND &lp_AcctSponSubAct
                   AND &lp_PrincipalInvestigator
                   AND &lp_AcctStatusDesc
                   AND &lp_SegStatusDesc
                   AND &lp_AcctRoot
                   AND &lp_AcctGroup
                   AND T568021.ROOT_KEY = T573797.DW_ROOT_KEY
                   AND T568021.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')
                   /*     AND T568061.FUND = '132626'  TEST */
                   AND T568061.FUND_KEY = T573797.DW_FUND_KEY
                   AND T568064.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')
                   AND T568064.TUB_KEY = T573797.DW_TUB_KEY
                   AND T568069.END_DATE_ACTIVE =
                           TO_DATE ('9999-12-31 00:00:00',
                                    'YYYY-MM-DD HH24:MI:SS')
                   AND T568069.ORG_KEY = T573797.DW_ORG_KEY
                   AND T568091.ADJUSTMENT_PERIOD_FLAG = 'N'
                   AND T568091.PERIOD_KEY = T573797.DW_PERIOD_KEY
                   AND T568091.PERIOD_KEY = :PeriodToKey
                   AND T573797.DW_PERIOD_KEY = :PeriodToKey
                   AND T573798.USER_UNIV_ID = :xdo_user_name
                   AND T568091.CURRENT_PERIOD_OFFSET < 1
                   AND 0 < T568091.PERIOD_KEY
                   AND T568046.OBJECT >= '4000'
                   and DGP.LATEST_OPEN_PERIOD_FLAG='Y'
                   AND T568040.DW_GL_FUND_KEY = BUDGETS.DW_FUND_KEY
AND T568040.DW_ACCOUNT_KEY = BUDGETS.DW_ACCOUNT_KEY
AND T568046.OBJECT_KEY = BUDGETS.DW_OBJECT_KEY
AND T568054.SUBACTIVITY_KEY = BUDGETS.DW_SUBACTIVITY_KEY
AND T568060.ACTIVITY_KEY = BUDGETS.DW_ACTIVITY_KEY
 AND T568021.ROOT_KEY = BUDGETS.DW_ROOT_KEY
 AND T568061.FUND_KEY = BUDGETS.DW_FUND_KEY
 AND T568064.TUB_KEY = BUDGETS.DW_TUB_KEY
 AND T568069.ORG_KEY = BUDGETS.DW_ORG_KEY
  AND DGP.PERIOD_KEY = BUDGETS.DW_PERIOD_KEY )
