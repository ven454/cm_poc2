                  + T568029.TOT_OBLIGATED_INDIRECT_AMT
                  + T568029.TOT_CARR_FWD_DIRECT_AMT
                  + T568029.TOT_CARR_FWD_INDIRECT_AMT
                      AS c29,
                    T568029.TOT_CARR_FWD_INDIRECT_AMT
                  + T568029.TOT_CARR_FWD_DIRECT_AMT
                      AS c30,
                  CASE T568046.TERA_OBJECT
                      WHEN 'T400' THEN 'Income'
                      WHEN 'T600' THEN 'Expense'
                      WHEN 'T898' THEN 'Expense'
                      WHEN 'T901' THEN 'Non-Oper'
                      WHEN 'T951' THEN 'Non-Oper'
                      WHEN 'T981' THEN 'Non-Oper'
                      ELSE 'OTHER'
                  END
                      AS c31,
                  SUBSTR (T568054.DESCRIPTION,
                          INSTR (T568054.DESCRIPTION, ',') + 1,
                          100)
                      AS c32,
