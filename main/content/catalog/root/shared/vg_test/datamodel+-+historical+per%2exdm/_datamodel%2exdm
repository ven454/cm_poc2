                          T568029.PI_FIRST_NAME)
                      AS c15,
                  T568029.RESPONSIBLE_ORG_CD             AS c16,
                  T568029.RESPONSIBLE_ORG_DESC           AS c17,
                  CONCAT (CONCAT (T568029.RESPONSIBLE_ORG_CD, ' '),
                          T568029.RESPONSIBLE_ORG_DESC)
                      AS c18,
                  T568029.RESPONSIBLE_TUB_CD             AS c19,
                  CONCAT (CONCAT (T568029.RESPONSIBLE_TUB_CD, ' '),
                          T568029.RESPONSIBLE_TUB_DESC)
                      AS c20,
                  T568029.SEG_SPONSOR_AWARD_NO           AS c21,
                  T568029.SEG_SPONSOR_NAME               AS c22,
