                                                          ,
                  ODSGM.OGM_ACCOUNTS               T568040 /* B2_OGM_ACCOUNTS (Grants - PER inner) */
                  LEFT OUTER JOIN
                   (Select dw_account_key, dw_segment_key, air_rate, air_effective_date 
		  from 
		  (
		  select dw_account_key, dw_segment_key, air_rate, air_effective_date, 
		  Max(air_Effective_Date) over (PARTITION by rates.dw_account_key) as maxDate 
		  from odsgm.ogm_account_idc_rates rates 
		  where 
		  rates.air_effective_date <= sysdate AND 
		  rates.dw_active_flag = 'Y' AND 
		  rates.AIR_DELETION_DATE is NULL
		  ) 
		  where air_effective_date = maxDate) T608824 On T568040.DW_ACCOUNT_KEY = T608824.DW_ACCOUNT_KEY,   
                  ODSGM.OGM_SEC_USER_RPT_SEGMENTS  T573798 /* B2_OGM_SEC_USER_RPT_SEGMENTS (PER Security Join) */
                                                          ,
                  HUDW.DGM_CURR_PER_GL_BALS_F_ALL_V T573799 /* D1_DGM_CURR_PER_GL_BALS_F_ALL_V (Grants - PER open view) */
