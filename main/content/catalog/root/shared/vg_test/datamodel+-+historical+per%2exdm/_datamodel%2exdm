                  SUM (T573799.EXP_ACTUAL_PJTD)          AS c3,
                  SUM (BUDGETS.SPONSORED_BUD_NET_PJTD_END) AS c4,
                  T568046.OBJECT                         AS c5,
                  T568046.DESCRIPTION                    AS c6,
                  T568060.ACTIVITY                       AS c7,
                  T568040.ACCOUNT_STATUS_DESC            AS c8,
                  T568040.ACCOUNT_TYPE_DESC              AS c9,
                  T568040.ACCT_ACCOUNT_GROUP_DESC        AS c10,
                  T568040.ACCT_END_DATE                  AS c11,
                  T568040.ACCT_START_DATE                AS c12,
                  T568029.SEG_ANTICIPATED_END_DATE       AS c13,
                  T568029.SEG_ANTICIPATED_START_DATE     AS c14,
                  CONCAT (CONCAT (T568029.PI_LAST_NAME, ', '),
                          T568029.PI_FIRST_NAME)
                      AS c15,
                  T568029.RESPONSIBLE_ORG_CD             AS c16,
                  T568029.RESPONSIBLE_ORG_DESC           AS c17,
