                  T568054.ACTIVITY,
                  T568054.DESCRIPTION,
                  T568054.SUBACTIVITY,
                  T568060.ACTIVITY,
                  T568061.FUND,
                  T568064.TUB,
                  T568069.ORG,
                  CASE T568046.TERA_OBJECT
                      WHEN 'T400' THEN 'Income'
                      WHEN 'T600' THEN 'Expense'
                      WHEN 'T898' THEN 'Expense'
                      WHEN 'T901' THEN 'Non-Oper'
                      WHEN 'T951' THEN 'Non-Oper'
                      WHEN 'T981' THEN 'Non-Oper'
                      ELSE 'OTHER'
                  END,
                  CASE
                      WHEN     T568046.FACILITY_ADMIN_COST_FLAG = 'N'
                           AND T568046.DIR_INDIR_COST_FLAG = 'Y'
                      THEN
                          CASE
                              WHEN T568046.TDC_OVERHEAD_FLAG = 'N'
                              THEN
                                  '*'
                              ELSE
                                  CASE
                                      WHEN     T568040.ACCT_IDC_BASIS_CD =
                                                   'MTDC'
                                           AND T568046.MTDC_OVERHEAD_FLAG =
                                                   'N'
                                      THEN
                                          '*'
                                      ELSE
                                          CASE
                                              WHEN     T568040.ACCT_IDC_BASIS_CD =
                                                           'HHSM'
                                                   AND T568046.HHSM_OVERHEAD_FLAG =
                                                           'N'
                                              THEN
                                                  '*'
                                          END
                                  END
                          END
                      ELSE
                          ''
                  END,
                  CONCAT (CONCAT (T568029.RESPONSIBLE_ORG_CD, ' '),
                          T568029.RESPONSIBLE_ORG_DESC),
                  CONCAT (CONCAT (T568029.RESPONSIBLE_TUB_CD, ' '),
                          T568029.RESPONSIBLE_TUB_DESC),
                  CONCAT (CONCAT (T568029.PI_LAST_NAME, ', '),
                          T568029.PI_FIRST_NAME),
                  CONCAT (CONCAT (T568046.OBJECT, ' '),
                          SUBSTR (T568046.NAME, 1, 30)),
                  CONCAT (
                      CONCAT (
