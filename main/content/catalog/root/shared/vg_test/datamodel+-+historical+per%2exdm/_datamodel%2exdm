   <valueSets>
      <valueSet id="AcctGroup" inuse="true">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[select	 "Dim - Financials"."Acct Account Group Cd" as "Acct Account Group Cd" 
 from	"Grants Management"."Dim - Financials" "Dim - Financials"
order by 1 ASC]]>
         </sql>
      </valueSet>
      <valueSet id="AcctStatusDesc" inuse="true">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[SELECT
  "Grants Management"."Dim - Accounts"."Acct Status Desc" ,
 cast("Grants Management"."Dim - Accounts"."Acct Status Cd" as CHAR)
FROM "Grants Management"
order by 1 ASC]]>
         </sql>
      </valueSet>
      <valueSet id="SegStatusDesc" inuse="true">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[select	 "Dim - Segments"."Segment Status Desc" ,

 cast("Dim - Segments"."Segment Status Cd" as CHAR)

 from	"Grants Management"
order by 1 ASC]]>
         </sql>
      </valueSet>
      <valueSet id="PrincipalInvestigator" inuse="true">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[select	 "Dim - Financials"."Principal Investigator" as "Principal Investigator" , 
CAST( "Dim - Financials"."Pi Univ ID" AS INT ) as "Pi Univ ID"
 from	"Grants Management"."Dim - Financials" "Dim - Financials"
order by 1 ASC]]>
         </sql>
      </valueSet>
      <valueSet id="AcctRoot" inuse="true">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[select	 "Dim - Financials"."Root" as "Root" 
 from	"Grants Management"."Dim - Financials" "Dim - Financials"
order by 1 ASC]]>
         </sql>
      </valueSet>
      <valueSet id="AcctSponSubAct" inuse="true">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[select	 "Dim - Sub Activities"."Subactivity" as "Subactivity" 
 from	"Grants Management"."Dim - Sub Activities" "Dim - Sub Activities"
order by 1 DESC]]>
         </sql>
      </valueSet>
      <valueSet id="AcctSponActivity" inuse="true">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[select	 "Dim - Activities"."Activity" as "Activity" 
 from	"Grants Management"."Dim - Activities" "Dim - Activities"
where  "Dim - Activities"."Activity"  >= '200000'
and "Dim - Activities"."Activity"  <= '499999' 
order by 1 asc]]>
         </sql>
      </valueSet>
      <valueSet id="AcctOrg" inuse="true">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[select	 "Grants Management"."Dim - Orgs"."Org" as "Org" 
 from	"Grants Management"."Dim - Orgs" "Dim - Orgs"
order by 1 ASC]]>
         </sql>
      </valueSet>
      <valueSet id="AcctTub" inuse="true">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[select	 "Grants Management"."Dim - Tubs"."Tub"  as "Tub" 
 from	 "Grants Management"."Dim - Tubs" "Dim - Tubs"
order by 1 ASC]]>
         </sql>
      </valueSet>
      <valueSet id="Acct Spon Fund" cache="true" inuse="true">
         <sql dataSourceRef="dwcen">
            <![CDATA[select	Fund as "Acct Fund" 
 from 	DWGL.DGL_COA_FUNDS_D
where  Fund  >= '100000' and  Fund <= '299999'
order by 1 ASC NULLS LAST]]>
         </sql>
      </valueSet>
      <valueSet id="Period_To_LOV" inuse="true">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[select	
"Dim - Periods"."Period Name"  as "Period Name",
 cast("Dim - Periods"."Period Key"  AS INT) as "Period Key" 
 from	"Grants Management"."Dim - Periods" "Dim - Periods"
WHERE "Dim - Periods"."Adjustment Period Flag" = 'N' 
order by   "Dim - Periods"."Calendar year"  desc , "Dim - Periods"."Calendar month"  desc]]>
         </sql>
      </valueSet>
      <valueSet id="Period_From_LOV" inuse="true">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[select	
"Dim - Periods"."Period Name"  as "Period Name",
 cast("Dim - Periods"."Period Key"  AS INT) as "Period Key" 
 from	"Grants Management"."Dim - Periods" "Dim - Periods"
WHERE "Dim - Periods"."Adjustment Period Flag" = 'N' 
order by   "Dim - Periods"."Calendar year"  desc , "Dim - Periods"."Calendar month"  desc]]>
         </sql>
      </valueSet>
   </valueSets>
