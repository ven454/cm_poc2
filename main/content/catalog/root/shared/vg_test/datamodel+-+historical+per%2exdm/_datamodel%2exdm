                            AS c24,
                        CASE
                            WHEN D1.c28 IS NOT NULL THEN D1.c28
                            WHEN D2.c28 IS NOT NULL THEN D2.c28
                        END
                            AS c25,
                        D1.c4                                          AS c26,
                        D1.c3                                          AS c27,
                        D1.c2                                          AS c28,
                        CASE
                            WHEN D1.c29 IS NOT NULL THEN D1.c29
                            WHEN D2.c29 IS NOT NULL THEN D2.c29
                        END
                            AS c29,
                        CASE
                            WHEN D1.c30 IS NOT NULL THEN D1.c30
                            WHEN D2.c30 IS NOT NULL THEN D2.c30
                        END
                            AS c30,
                        CASE
                            WHEN D1.c29 IS NOT NULL THEN D1.c29
                            WHEN D2.c29 IS NOT NULL THEN D2.c29
                        END
                            AS c31,
                        NVL (D2.c3, 0) - NVL (D2.c1, 0)                AS c32,
                        CASE
                            WHEN D1.c31 IS NOT NULL THEN D1.c31
                            WHEN D2.c31 IS NOT NULL THEN D2.c31
                        END
                            AS c33,
                        CASE
                            WHEN D1.c32 IS NOT NULL THEN D1.c32
                            WHEN D2.c32 IS NOT NULL THEN D2.c32
                        END
                            AS c34,
                        NVL (D2.c1 / NULLIF (D2.c3, 0) * 100 / 100, 0) AS c35,
                        CASE
                            WHEN D1.c33 IS NOT NULL THEN D1.c33
                            WHEN D2.c33 IS NOT NULL THEN D2.c33
                        END
                            AS c36,
                        CASE
                            WHEN D1.c34 IS NOT NULL THEN D1.c34
                            WHEN D2.c34 IS NOT NULL THEN D2.c34
                        END
                            AS c37,
                        CASE
                            WHEN D1.c35 IS NOT NULL THEN D1.c35
                            WHEN D2.c35 IS NOT NULL THEN D2.c35
                        END
                            AS c38,
                        D1.c1                                          AS c39,
                        CASE
                            WHEN D1.c36 IS NOT NULL THEN D1.c36
                            WHEN D2.c36 IS NOT NULL THEN D2.c36
                        END
                            AS c40,
                        CASE
                            WHEN D1.c37 IS NOT NULL THEN D1.c37
                            WHEN D2.c37 IS NOT NULL THEN D2.c37
                        END
                            AS c41,
                        CASE
                            WHEN D1.c38 IS NOT NULL THEN D1.c38
                            WHEN D2.c38 IS NOT NULL THEN D2.c38
                        END
                            AS c42,
                        CASE
                            WHEN CASE
                                     WHEN D1.c31 IS NOT NULL THEN D1.c31
                                     WHEN D2.c31 IS NOT NULL THEN D2.c31
                                 END = 'Non-Oper'
                            THEN
                                D2.c4
                            ELSE
                                0
                        END
                            AS c43,
                        CASE
                            WHEN CASE
                                     WHEN D1.c31 IS NOT NULL THEN D1.c31
                                     WHEN D2.c31 IS NOT NULL THEN D2.c31
                                 END = 'Income'
                            THEN
                                D2.c4
                            ELSE
                                0
                        END
                            AS c44,
                        CASE
                            WHEN D1.c39 IS NOT NULL THEN D1.c39
                            WHEN D2.c39 IS NOT NULL THEN D2.c39
                        END
                            AS c45,
                        D2.c2                                          AS c46,
                        D2.c1                                          AS c47,
                        CASE
                            WHEN D1.c40 IS NOT NULL THEN D1.c40
                            WHEN D2.c40 IS NOT NULL THEN D2.c40
                        END
                            AS c48,
                        CASE
                            WHEN D1.c41 IS NOT NULL THEN D1.c41
                            WHEN D2.c41 IS NOT NULL THEN D2.c41
                        END
                            AS c49,
                        CASE
                            WHEN D1.c42 IS NOT NULL THEN D1.c42
                            WHEN D2.c42 IS NOT NULL THEN D2.c42
                        END
                            AS c50,
                        ROW_NUMBER ()
                        OVER (
                            PARTITION BY CASE
                                             WHEN D1.c5 IS NOT NULL
                                             THEN
                                                 D1.c5
                                             WHEN D2.c5 IS NOT NULL
                                             THEN
                                                 D2.c5
                                         END,
