                                    WHEN D2.c8 IS NOT NULL THEN D2.c8
                                END ASC,
                                CASE
                                    WHEN D1.c9 IS NOT NULL THEN D1.c9
                                    WHEN D2.c9 IS NOT NULL THEN D2.c9
                                END ASC,
                                CASE
                                    WHEN D1.c10 IS NOT NULL THEN D1.c10
                                    WHEN D2.c10 IS NOT NULL THEN D2.c10
                                END ASC,
                                CASE
                                    WHEN D1.c11 IS NOT NULL THEN D1.c11
                                    WHEN D2.c11 IS NOT NULL THEN D2.c11
                                END ASC,
                                CASE
