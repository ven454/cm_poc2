               <element name="CS_Total_Direct_Costs_PerTrx" value="${G_3.CS_Direct_Costs_Subj_Ovr_PerTrx+ G_3.CS_Direct_Costs_Subj_NotOvr_PerTrx}" label="CS_Total_Direct_Costs_PerTrx" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
               <element name="CS_FACost_RemBud" value="${G_3.CS_FACost_Bud-G_3.CS_FACost_SubAct}" label="CS_FACost_RemBud" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
