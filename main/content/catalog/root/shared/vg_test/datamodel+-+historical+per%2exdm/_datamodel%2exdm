               <element name="CS_FACost_PerExp" value="${G_3.CS_FACost_SubAct/G_3.CS_FACost_Bud}" label="CS_FACost_PerExp" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
               <element name="CS_Direct_Costs_NotSubj_Ovr_PerExp" value="${NVL((G_3.CS_Direct_Costs_NotSubj_Ovr_SubAct_ToDate / G_3.CS_Direct_Costs_NotSubj_Ovr_Bud),0)}" label="CS_Direct_Costs_NotSubj_Ovr_PerExp" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
               <element name="CS_Total_Expenses_PerTrx" value="${G_3.CS_Total_Direct_Costs_PerTrx+G_3.CS_FACost_PeriodTrx}" label="CS_Total_Expenses_PerTrx" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
               <element name="CS_Total_Expenses_Bud" value="${G_3.CS_Total_Direct_Costs_Bud+G_3.CS_FACost_Bud}" label="CS_Total_Expenses_Bud" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
               <element name="CS_Total_Expenses_SubAct" value="${G_3.CS_Total_Direct_Costs_SubAct_ToDate + G_3.CS_FACost_SubAct}" label="CS_Total_Expenses_SubAct" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
               <element name="CS_Total_Expenses_RemBud" value="${G_3.CS_Total_Direct_Costs_RemBud + G_3.CS_FACost_RemBud}" label="CS_Total_Expenses_RemBud" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
               <element name="CS_Total_Direct_Costs_PerExp" value="${NVL((G_3.CS_Total_Direct_Costs_SubAct_ToDate/ G_3.CS_Total_Direct_Costs_Bud),0)}" label="CS_Total_Direct_Costs_PerExp" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
               <element name="CS_Total_Expenses_PerExp" value="${NVL((G_3.CS_Total_Expenses_SubAct/ G_3.CS_Total_Expenses_Bud),0)}" label="CS_Total_Expenses_PerExp" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
               <element name="CS_Balance_PerTrx" value="${G_3.CS_Total_Expenses_PerTrx + G_3.CS_Total_Income_PerTrx +  G_3.CS_Non_Oper_PerTrx}" label="CS_Balance_PerTrx" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
               <element name="CS_Balance_SubAct" value="${G_3.CS_Total_Expenses_SubAct+G_3.CS_Total_Income_SubActToDate + G_3.CS_Non_Oper_SubAct}" label="CS_Balance_SubAct" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
