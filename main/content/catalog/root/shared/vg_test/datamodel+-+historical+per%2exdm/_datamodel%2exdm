                                          CONCAT (CONCAT (T568061.FUND, '-'),
                                                  T568054.ACTIVITY),
                                          '-'),
                                      T568054.SUBACTIVITY),
                                  '-'),
                              T568064.TUB),
                          '-'),
                      T568069.ORG),
                  SUBSTR (T568054.DESCRIPTION,
                          INSTR (T568054.DESCRIPTION, ',') + 1,
                          100)),
     SAWITH2
     AS (SELECT D1.c5  AS c5,
