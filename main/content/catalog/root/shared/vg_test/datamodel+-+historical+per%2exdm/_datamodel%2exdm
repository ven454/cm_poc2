                  T568029.RESPONSIBLE_TUB_CD             AS c19,
                  CONCAT (CONCAT (T568029.RESPONSIBLE_TUB_CD, ' '),
                          T568029.RESPONSIBLE_TUB_DESC)
                      AS c20,
                  T568029.SEG_SPONSOR_AWARD_NO           AS c21,
                  T568029.SEG_SPONSOR_NAME               AS c22,
                  T568061.FUND                           AS c23,
                  CONCAT (CONCAT (T568046.OBJECT, ' '),
                          SUBSTR (T568046.NAME, 1, 30))
                      AS c24,
                  T568069.ORG                            AS c25,
                  T568054.DESCRIPTION                    AS c26,
                  T568054.SUBACTIVITY                    AS c27,
                  T568064.TUB                            AS c28,
                    T568029.TOT_OBLIGATED_DIRECT_AMT
                  + T568029.TOT_OBLIGATED_INDIRECT_AMT
                  + T568029.TOT_CARR_FWD_DIRECT_AMT
                  + T568029.TOT_CARR_FWD_INDIRECT_AMT
                      AS c29,
                    T568029.TOT_CARR_FWD_INDIRECT_AMT
                  + T568029.TOT_CARR_FWD_DIRECT_AMT
                      AS c30,
                  CASE T568046.TERA_OBJECT
                      WHEN 'T400' THEN 'Income'
                      WHEN 'T600' THEN 'Expense'
                      WHEN 'T898' THEN 'Expense'
                      WHEN 'T901' THEN 'Non-Oper'
                      WHEN 'T951' THEN 'Non-Oper'
                      WHEN 'T981' THEN 'Non-Oper'
                      ELSE 'OTHER'
                  END
                      AS c31,
                  SUBSTR (T568054.DESCRIPTION,
                          INSTR (T568054.DESCRIPTION, ',') + 1,
                          100)
                      AS c32,
                  CASE
                      WHEN     T568046.FACILITY_ADMIN_COST_FLAG = 'N'
                           AND T568046.DIR_INDIR_COST_FLAG = 'Y'
                      THEN
                          CASE
                              WHEN T568046.TDC_OVERHEAD_FLAG = 'N'
                              THEN
                                  '*'
                              ELSE
                                  CASE
                                      WHEN     T568040.ACCT_IDC_BASIS_CD =
                                                   'MTDC'
                                           AND T568046.MTDC_OVERHEAD_FLAG = 'N'
                                      THEN
                                          '*'
                                      ELSE
                                          CASE
                                              WHEN     T568040.ACCT_IDC_BASIS_CD =
                                                           'HHSM'
                                                   AND T568046.HHSM_OVERHEAD_FLAG =
                                                           'N'
                                              THEN
                                                  '*'
                                          END
                                  END
                          END
                      ELSE
                          ''
                  END
                      AS c33,
                  T568046.FACILITY_ADMIN_COST_FLAG       AS c34,
                  T568046.NON_OPERATING_TRANSFER_FLAG    AS c35,
                  CONCAT (
